var _utils = require("../utils");

var mockGetColor = _utils.getColor;
describe('getColor for tag label', function () {
  it('should return black color when background color is light', function () {
    expect(mockGetColor('eedd82')).toBe('black');
  });
  it('should return white color when background color is dark', function () {
    expect(mockGetColor('A9A9A9')).toBe('white');
  });
});
