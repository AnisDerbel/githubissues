'use strict';

var ES5ToInteger = require('../5/ToInteger');

var ToNumber = require('./ToNumber');

module.exports = function ToInteger(value) {
  var number = ToNumber(value);

  if (number !== 0) {
    number = ES5ToInteger(number);
  }

  return number === 0 ? 0 : number;
};
